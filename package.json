{
  "name": "lambda-bag",
  "version": "0.0.14",
  "description": "Functional utility toolbelt for vanillaJS",
  "main": "lamba-bag.js",
  "scripts": {
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
    "check-types": "tsc",
    "docs": "./node_modules/.bin/esdoc",
    "open:docs": "open docs/index.html",
    "lint": "eslint",
    "test:coverage": "npm run clean && jest --coverage",
    "test": "jest",
    "build:prod": "npm install && npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack && ./node_modules/.bin/esdoc"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juanrodriguezarc/lambda-bag"
  },
  "keywords": [
    "functional",
    "javascript",
    "toolbelt",
    "utilities",
    "observers",
    "state"
  ],
  "author": "Juan Rodriguez Arce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanrodriguezarc/lamda-bag/issues"
  },
  "homepage": "https://github.com/juanrodriguezarc/lamda-bag#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.5.5",
    "@babel/types": "^7.6.1",
    "@types/jest": "^24.0.18",
    "acorn": "^7.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^6.3.0",
    "fetch": "^1.1.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "puppeteer": "^1.20.0",
    "shx": "^0.3.2",
    "ts-loader": "^6.1.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
