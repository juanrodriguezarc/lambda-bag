{
  "name": "lambda-bag",
  "version": "0.0.4",
  "description": "Functional utility toolbelt for vanillaJS",
  "main": "lamba-bag.js",
  "scripts": {
    "build": "webpack",
    "check-types": "tsc",
    "docs": "./node_modules/.bin/esdoc",
    "lint": "eslint",
    "start": "webpack-dev-server --inline",
    "test:coverage": "jest --coverage",
    "test": "jest",
    "build:prod": "npm install && webpack -p --config ./webpack.config.prod.js && ./node_modules/.bin/esdoc"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanrodriguezarc/lamda-bag.git"
  },
  "keywords": [
    "functional",
    "javascript",
    "toolbelt",
    "utilities"
  ],
  "author": "Juan Rodriguez Arce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanrodriguezarc/lamda-bag/issues"
  },
  "homepage": "https://github.com/juanrodriguezarc/lamda-bag#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^6.3.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "puppeteer": "^1.19.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.7.2",
    "acorn": "^7.1.0"
  }
}
